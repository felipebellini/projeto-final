# Criando listas

Quadrantes = [[0, 0], [0, 6], [0, 12], [6, 0], [6, 6], [6, 12], [12, 0], [12, 6], [12, 12]]
linha = 0
coluna = 0
posicao = 0
for elemento in Quadrantes:
    linha = elemento[0]
    coluna = elemento[1]
    for b in range(0, 9):
        if b <= 2:
            posicao = b
            elemento.append(Button(linha, coluna, posicao))
            coluna += 1
            posicao += 1
        elif b == 3:
            linha += 1
            coluna = elemento[1]
            posicao = b
            elemento.append(Button(linha, coluna, posicao))
            coluna += 1

            posicao += 1

        elif b <= 5:
            posicao = b
            elemento.append(Button(linha, coluna, posicao))
            coluna += 1
            posicao += 1

        elif b == 6:
            linha += 1
            coluna = elemento[1]
            posicao = b
            elemento.append(Button(linha, coluna, posicao))
            coluna += 1
            posicao += 1

        elif b <= 8:
            posicao = b
            elemento.append(Button(linha, coluna, posicao))
            coluna += 1
            posicao += 1

# =============================================================================================================================#
# Criando os labels grande
labelGrandeH1 = tk.Label(tela_de_jogo, height=3, bg="black")
labelGrandeH1.grid(row=5, column=0)

labelGrandeH2 = tk.Label(tela_de_jogo, height=3, bg="black")
labelGrandeH2.grid(row=11, column=0)

labelGrandeV1 = tk.Label(tela_de_jogo, width=6, bg="black")
labelGrandeV1.grid(row=0, column=5)

labelGrandeV2 = tk.Label(tela_de_jogo, width=6, bg="black")
labelGrandeV2.grid(row=0, column=11)

