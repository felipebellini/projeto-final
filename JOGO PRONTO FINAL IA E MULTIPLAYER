                    elif self.conteudo_label.get() == "O":
                        label = tk.Label(tela_de_jogo, width=32, height=14)
                        label.place(x=565, y=550)
                        label = tk.Label(tela_de_jogo, text="O", fg="red", font="Arial 141")
                        label.place(x=605, y=550)
                        if self.posicao == self.quadrante:
                            pquad = -1
                        QuadV[self.quadrante] = 2


            elif Quadrantes[self.quadrante][5].conteudo_label.get() == Quadrantes[self.quadrante][
                6].conteudo_label.get() and Quadrantes[self.quadrante][5].conteudo_label.get() == \
                    Quadrantes[self.quadrante][7].conteudo_label.get() and Quadrantes[self.quadrante][
                5].conteudo_label.get() == self.conteudo_label.get():

                if self.quadrante == 0:
                    if self.conteudo_label.get() == "X":
                        label = tk.Label(tela_de_jogo, width=32, height=14)
                        label.place(x=3, y=4)
                        label = tk.Label(tela_de_jogo, text="X", fg="blue", font="Arial 141")
                        label.place(x=50, y=4)
                        if self.posicao == self.quadrante:
